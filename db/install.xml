<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="enrol/approvalenrol/db" VERSION="20250111" COMMENT="XMLDB file for Moodle enrol/approvalenrol"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="user_enrol_approval_requests" COMMENT="This table contains list of user requested for course enrolment.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="firstname" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="lastname" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="email" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="foreign" TYPE="foreign" FIELDS="email" REFTABLE="user" REFFIELDS="email"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
